  <div class="container">    
    <table class="table align-middle table-dark table-hover text-center mt-4 caption-top"  id="tablaFlujo">
      <caption class="h3">Flujo de Efectivo</caption>
      <thead>
        <tr class="bg-primary text-white">
          <th scope="col"></th>
          <th>Mes</th>
          <th id="total" scope="col">TOTAL</th>
        </tr>
      </thead>
      <tbody>
          <tr>
            <td>TITULOS</td>
            <td><input class="form-control text-center " type="text" placeholder="tituloFE [STRING]" id="tituloFE"  required></td>
            <td></td>
          </tr>
          <tr>
            <td>INGRESOS</td>
            <td><input class="form-control text-center " type="number" placeholder="ingresosFE=[DECIMAL]" id="ingresosFE" step="0.01" oninput="calcular()" required></td>
            <td><input class="form-control text-center " type="number" placeholder="sumIngresos.FE" id="sumIngresosFE" disabled></td>
          </tr>
          <tr>
            <td>EGRESOS</td>
            <td><input class="form-control text-center " type="number" placeholder="egresosFE=totCD.mes+totGA.mes" id="egresosFE" value="10.00" step="0.01" oninput="calcular()" disabled></td>
            <td><input class="form-control text-center " type="number" placeholder="sumEgresos.FE" id="sumEgresosFE" disabled></td>
          </tr>
          <tr>
            <td>TOTAL</td>
            <td><input class="form-control text-center " type="number" placeholder="totalFE=ingresosFE-egresosFE[-R]" id="totalFE" step="0.01" disabled></td>
            <td><input class="form-control text-center " type="number" placeholder="sumTotal.FE" id="sumTotalFE" disabled></td>
          </tr>
      </tbody>
      <tfoot>
        <tr class="bg-primary text-white">
          <td scope="col">ACUMULADO</th>
          <td><input class="form-control text-center " type="number" placeholder="acumuladoFE+=totalFE" id="acumuladoFe" step="0.01" disabled></td>
          <td scope="col"></th>
        </tr>
      </tfoot>
    </table>
    <div class="d-flex justify-content-evenly">
      <button type="button" id="agregarMes" class="btn btn-primary col-md-4"><i class='bx bxs-plus-circle'></i> AGREGAR MES</a>
      <button type="button" id="eliminarMes" class="btn btn-danger col-md-4"><i class='bx bxs-trash-alt'></i> ELIMIAR MES</a>
    </div>
  </div>